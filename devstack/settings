# Settings for autoscaler-interattivo-plugin
AUTOSCALER_DEST=${AUTOSCALER_DEST:-$DEST/autoscaler-interattivo-plugin}
AUTOSCALER_FLAVOR_SMALL=${AUTOSCALER_FLAVOR_SMALL:-as.small}
AUTOSCALER_FLAVOR_MEDIUM=${AUTOSCALER_FLAVOR_MEDIUM:-as.medium}
AUTOSCALER_IMAGE_NAME=${AUTOSCALER_IMAGE_NAME:-cirros-0.6.3-x86_64-disk}
AUTOSCALER_SERVER_NAME=${AUTOSCALER_SERVER_NAME:-auto-vm-1}
AUTOSCALER_CREATE_VM_ON_STACK=${AUTOSCALER_CREATE_VM_ON_STACK:-False}
AUTOSCALER_RUN_DEMO=${AUTOSCALER_RUN_DEMO:-False}
enable_service autoscaler-interattivo-plugin
